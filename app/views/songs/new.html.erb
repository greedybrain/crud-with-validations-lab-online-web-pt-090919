<%= form_for(@song) do |f| %>

     <%= f.label "Title" %><br>
     <%= f.text_field :title%><br><br>

     <%= f.label "Released" %><br>
     <%= f.radio_button :released, 'Yes', id: "song_released", checked: true %>Yes
     <%= f.radio_button :released, 'No' %>No<br><br>

     <%= f.label "Release Year" %><br>
     <%= f.text_field :release_year %><br><br>

     <%= f.label "Artist Name" %><br>
     <%= f.text_field :artist_name %><br><br>

     <%= f.label "Genre" %><br>
     <%= f.text_field :genre%><br><br>

     <%= f.submit %>

<% end %>